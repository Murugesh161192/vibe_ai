{
  "name": "vibe-ai-backend",
  "version": "1.0.0",
  "description": "Fastify backend for GitHub repository vibe score analysis",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "build": "echo 'No build step required for backend'",
    "dev": "nodemon src/server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "test:watch": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "lint": "eslint src/",
    "setup-token": "node setup-token.js",
    "debug": "node debug-test.js",
    "test-token": "node test-token.js"
  },
  "keywords": [
    "github",
    "repository",
    "analysis",
    "vibe-score",
    "fastify",
    "api",
    "cognizant",
    "vibe-coding"
  ],
  "author": "Cognizant Vibe Coding 2025 Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/env": "^4.2.1",
    "@fastify/rate-limit": "^8.0.0",
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
