{
  "name": "vibe-github-analyzer",
  "version": "1.0.0",
  "description": "A powerful web application that analyzes GitHub repositories and provides comprehensive insights into code quality, community health, and project vibes using advanced metrics and AI-powered analysis",
  "main": "start.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "node server.js",
    "start:backend": "cd backend && npm start",
    "build": "npm install --production",
    "setup:deployment": "node setup-deployment.js"
  },
  "keywords": [
    "github",
    "repository",
    "analysis",
    "vibe-score",
    "fastify",
    "react",
    "api",
    "ai-insights",
    "gemini",
    "cognizant",
    "vibe-coding-2025",
    "code-quality",
    "analytics"
  ],
  "author": "Cognizant Vibe Coding 2025 Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Murugesh161192/vibe_ai.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/env": "^4.2.1",
    "@fastify/rate-limit": "^8.0.0",
    "@google/generative-ai": "^0.2.1",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  }
} 